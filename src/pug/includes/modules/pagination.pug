- var pages = ['1', '2', '3', '4', '...', '10', '11', '12'];

.pagination__wrapper
  a.pagination__link.is-active(href="#")
    svg(width='20' height='20' viewbox='0 0 24 24' fill='none' xmlns='http://www.w3.org/2000/svg')
      path(d='M21.75 12H1.75' stroke='#48A490' stroke-width='3' stroke-linecap='round' stroke-linejoin='round')
      path(d='M11.75 2L1.75 12L11.75 22' stroke='#48A490' stroke-width='3' stroke-linecap='round' stroke-linejoin='round')
  each link in pages
    a.pagination__link(href="#") !{link}
  a.pagination__link.is-active(href="#")
    svg(width='20' height='20' viewbox='0 0 24 24' fill='none' xmlns='http://www.w3.org/2000/svg')
      path(d='M2 12H22' stroke='#043C53' stroke-width='3' stroke-linecap='round' stroke-linejoin='round')
      path(d='M12 2L22 12L12 22' stroke='#043C53' stroke-width='3' stroke-linecap='round' stroke-linejoin='round')
